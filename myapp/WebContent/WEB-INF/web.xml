    <servlet-class>controller.InsertMessageController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InsertMessageController</servlet-name>
    <url-pattern>/insert.do</url-pattern>
  </servlet-mapping>
</web-app>